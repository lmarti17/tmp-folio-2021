{
  "name": "parcel-plate",
  "version": "1.0.0",
  "description": "Parcel starter including scss and hmr",
  "scripts": {
    "dev": "parcel ./src/index.html --port 3000",
    "clear": "rm -rf ./dist node_modules/.cache ./.parcel-cache",
    "build": "npm run clear && parcel build src/index.html  --public-url ./dist/ --no-cache --no-source-maps",
    "build-vercel": "npm run clear && parcel build src/index.html --no-cache --no-source-maps"
  },
  "author": "Lucas Martin <contact@lucasmartin.fr>",
  "license": "ISC",
  "engines": {
    "node": ">=10.0.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "fitty": "^2.3.5",
    "gsap": "file:gsap-bonus.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@parcel/packager-raw-url": "^2.0.0-rc.0",
    "@parcel/transformer-glsl": "^2.0.0-rc.0",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "@parcel/transformer-webmanifest": "^2.0.0-rc.0",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.2",
    "parcel": "^2.0.0-rc.0",
    "posthtml-modules": "^0.7.4"
  }
}
